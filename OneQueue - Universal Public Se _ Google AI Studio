.mat-mdc-radio-button .mdc-radio:hover > .mdc-radio__native-control:enabled:checked + .mdc-radio__background > .mdc-radio__outer-circle { border-color: var(--mat-radio-selected-hover-icon-color, var(--mat-sys-primary)); }

