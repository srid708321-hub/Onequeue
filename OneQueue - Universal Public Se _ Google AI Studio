.mat-mdc-radio-button .mdc-radio { display: inline-block; position: relative; flex: 0 0 auto; box-sizing: content-box; width: 20px; height: 20px; cursor: pointer; will-change: opacity, transform, border-color, color; padding: calc((var(--mat-radio-state-layer-size, 40px) - 20px)/2); }

